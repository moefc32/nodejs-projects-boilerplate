{
    "compilerOptions": {
        "target": "ES2021",
        "module": "ES2022",
        "moduleResolution": "node",
        "types": ["bun-types"],
        "allowJs": true,
        "checkJs": false,
        "noEmit": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true
    }
}
